{
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rx-immer-react",
  "author": "xiaojunjiang",
  "module": "dist/rx-immer-react.esm.js",
  "size-limit": [
    {
      "path": "dist/rx-immer-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rx-immer-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.12",
    "husky": "^8.0.1",
    "prettier": "^2.7.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "react": ">=17",
    "rx-immer": ">=1.0.0"
  }
}
