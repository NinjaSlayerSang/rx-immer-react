{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rx-immer-react",
  "author": "xiaojunjiang",
  "module": "dist/rx-immer-react.esm.js",
  "size-limit": [
    {
      "path": "dist/rx-immer-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rx-immer-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.3",
    "@types/react": "^17.0.33",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "size-limit": "^6.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "lodash": ">=4",
    "lodash-es": ">=4",
    "react": ">=16",
    "rx-immer": ">=0.1.2"
  }
}
